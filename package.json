{
  "name": "cli-input",
  "version": "0.1.13",
  "description": "Prompt and user input library.",
  "author": "muji <noop@xpm.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cli-kit/cli-input"
  },
  "bugs": {
    "url": "https://github.com/cli-kit/cli-input/issues"
  },
  "keywords": [
    "cli",
    "user",
    "input",
    "readline",
    "prompt",
    "read",
    "interactive",
    "stdin"
  ],
  "dependencies": {
    "async": "~1.5.2",
    "cli-native": "~1.0.0",
    "cli-util": "~1.1.27",
    "touch": "~1.0.0"
  },
  "devDependencies": {
    "async-validate": "~0.13.0",
    "chai": "~3.4.1",
    "istanbul": "~0.4.2",
    "mocha": "~2.3.4",
    "mute-stream": "0.0.5",
    "ttycolor": "~0.9.0"
  },
  "engine": [
    "node >= 0.10.21"
  ],
  "scripts": {
    "readme": "mdp --force -v",
    "pretest": "rm -rfv ./target && mkdir -p ./target",
    "test": "mocha -u bdd --recursive --reporter list -A test/global test/spec",
    "cover": "istanbul cover _mocha -- -u bdd --recursive --reporter list -A test/global test/spec"
  },
  "config": {},
  "mdp": {
    "title": "Prompt",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/cli-kit/cli-input",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "features.md",
          "documentation.md",
          "usage.md",
          "example.md",
          "developer.md"
        ]
      }
    ]
  }
}
