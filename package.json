{
  "name": "cli-input",
  "version": "0.1.8",
  "description": "Prompt and user input library.",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/cli-input"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/cli-input/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/freeformsystems/cli-input/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "cli",
    "user",
    "input",
    "readline",
    "prompt",
    "read",
    "interactive",
    "stdin"
  ],
  "dependencies": {
    "async": "~0.9.0",
    "cli-native": "~1.0.0",
    "cli-util": "~1.1.27",
    "touch": "0.0.3",
    "ttycolor": "~0.8.14"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "istanbul": "~0.2.4",
    "mocha": "~1.17.0",
    "mute-stream": "0.0.4",
    "ttycolor": "^0.8.10"
  },
  "engine": [
    "node >= 0.10.21"
  ],
  "scripts": {
    "readme": "mdp --force -v",
    "pretest": "rm -rfv ./target && mkdir -p ./target",
    "test": "istanbul cover _mocha -- -u bdd --recursive --reporter list -A test/spec/"
  },
  "config": {},
  "mdp": {
    "title": "Prompt",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/cli-input",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "features.md",
          "documentation.md",
          "usage.md",
          "example.md",
          "developer.md"
        ]
      }
    ]
  }
}
